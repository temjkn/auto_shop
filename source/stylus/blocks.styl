.b-complect-item
	display block
	text-decoration none
	padding 16px 20px
	background-position center
	background-repeat no-repeat
	background-size cover
	overflow hidden
	position relative
	&:hover
		@media $min_md
			&:before
				background-color rgba(176, 203, 31, .85)
			.b-complect-item
				&__subtitle,
				&__title,
				&__price
					color #242227
				&__top
					border-color #242227
			.b-complect-list
				li
					color #242227
					&:before
						sprite($s-complect-list-hover)
	&:before
		content ''
		position absolute
		stick()
		background-color rgba(26, 19, 14, .65)
		transition .5s background-color
	&__top
		display flex
		justify-content space-between
		align-items flex-end
		padding-bottom 15px
		border-bottom 1px solid #fff
		margin-bottom 15px
		position relative
		z-index 1
		transition .5s border-color
	&__top-l
		text-align left
	&__subtitle
		color #fff
		margin-bottom 8px
		transition .5s color
		&:last-child
			margin-bottom 0
	&__title
		display block
		font-size 22px
		font-weight 500
		color #b0cb1f
		transition .5s color
	&__price
		font-size 22px
		font-weight 500
		color #fff
		transition .5s color
	.b-complect-list
		position relative
		z-index 1
		
.b-news-list-item
	&__img-wrap
		display block
		text-decoration none
	&__img
		width 100%
		height auto
	&__content
		padding 24px 20px 18px 20px
	&__title
