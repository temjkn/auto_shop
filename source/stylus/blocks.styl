.b-advantages
	width 100%
	font-size 0px
	display table
	table-layout fixed
	margin 50px 0 40px
	position relative
	&.owl-carousel
		margin-bottom 100px
		.b-advantages__item
			height auto
		.owl-item
			img
				display inline-block
				width auto
	&__item
		display table-cell
		width 16.6%
		height 145px
		text-align center
		white-space nowrap
		&:nth-child(even)
			vertical-align bottom
		&:nth-child(odd)
