		p
			color #4a4a4a
			font-size 18px
			// font-family PFDinDisplayPro-Medium
			font-weight 500
	.owl-dots
		position absolute
		margin-top 55px
	.owl-dot
		display inline-block
		margin-right 10px
		height 8px
		width 8px
		border-radius 50%
		border 2px solid #b0cb1f
		&:last-child
			margin-right 0px
		&.active
			background-color #b0cb1f
	.owl-nav
		// margin-left 50px
		position absolute
		margin-top 30px
		right 0
		font-size 0
	.owl-prev, .owl-next
		display inline-block
		vertical-align top
		width 58px
		height 58px
		border 2px solid #b0cb1f
		position relative
		&:after
			content ''
			position absolute
			align()
		&:hover
			background-color #b0cb1f
	.owl-prev
		border-right 1px solid #b0cb1f
		&:after
			sprite($s-arr-l-green)
		&:hover
			&:after
				sprite($s-arr-l-white)
	.owl-next
		border-left 1px solid #b0cb1f
		&:after
			sprite($s-arr-r-green)
		&:hover
			&:after
				sprite($s-arr-r-white)

